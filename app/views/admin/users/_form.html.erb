<%= form_for([:admin,@user]) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label :sign_in_count %><br />
    <%= f.text_area :sign_in_count %>
  </div>

  <div class="field">
    <%= f.label :last_sign_in_ip %><br />
    <%= f.text_area :last_sign_in_ip %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
